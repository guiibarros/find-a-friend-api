// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Org {
  id            String @id @default(uuid())
  title         String
  phone         String @unique
  password_hash String
  cep           String
  address       String
  uf            String
  city          String

  @@map("orgs")
}
